{
  "builds": [
    {
      "src": "weatherproject/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11.3"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "weatherproject/wsgi.py"
    }
  ]
}
{
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "/index.wsgi" }
  ]
}